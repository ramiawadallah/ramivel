{
    "name": "ramivel/ramivel",
    "description": "Laravel package",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel multiauth",
        "multiauth",
        "admin auth",
        "multi auth"
    ],
    "authors": [{
        "name": "Rami Awadallah",
        "email": "rami.moh.awadallah@gmail.com",
        "website": "ramiawadallah.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "laravelcollective/html":"^5.8.0",
        "ramiawadallah/presenter": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.8@dev",
        "orchestra/database": "^3.8@dev",
        "illuminate/support": "5.8.*",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Ramivel\\Multiauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ramivel\\Multiauth\\Tests\\":"tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ramivel\\Multiauth\\MultiauthServiceProvider"
            ]
        }
    }
}
